{
    "name": "digitalcloud/eloquent-custom-actions",
    "description": "Laravel Eloquent Custom Actions, make it easier to simulate eloquent events for custom actions.",
    "keywords": [
        "laravel",
        "eloquent",
        "event",
        "listener",
        "package"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCloud\\EloquentActions\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
